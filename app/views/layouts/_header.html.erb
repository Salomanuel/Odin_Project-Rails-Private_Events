<div class="header">
  <ul>
  	<% if logged_in? %>        
      <li><%= link_to "users",  users_path %></li>
      <li><%= link_to "events", events_path %></li>
      <li class=system><%= link_to "logout",  logout_path, method: :delete %></li>
      <li class=system><%= link_to "profile of #{@current_user.name}", current_user %></li>            
    <% else %>
      <li><%= link_to "signup",   signup_path %></li>
      <li class=system><%= link_to "login",    login_path %></li>
  	<% end %>
  </ul>
</div>